%section
  %h2.foo= t('.title')

  = button_link_to("Back to #{@event.name}", @event)

  %table#permit_report_table
    %tr
      %th= Reply.human_attribute_name("name") 
      %th= Reply.human_attribute_name("pid") 
  
    - @replies.ascend_by_name.each do |reply|
      %tr
        %td= reply.name
        %td= reply.pid
  

